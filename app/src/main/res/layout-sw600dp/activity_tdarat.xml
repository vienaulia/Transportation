<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backroad"
    android:screenOrientation="landscape"
    tools:context="com.example.vien.transportasi.TdaratActivity">

<RelativeLayout
    android:layout_width="576dp"
    android:layout_height="wrap_content"
    tools:layout_editor_absoluteY="0dp"
    tools:layout_editor_absoluteX="8dp"
    android:id="@+id/relativeLayout"
    android:layout_alignParentTop="true"
    android:layout_toLeftOf="@+id/imageambulan1"
    android:layout_toStartOf="@+id/imageambulan1">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scrollbars="none"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp"
        android:id="@+id/scrollView2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <ImageButton
                android:id="@+id/imageambulance"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:background="@drawable/ambulance" />

            <ImageButton
                android:id="@+id/imageangkot"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:background="@drawable/angkot" />
            <ImageButton
                android:id="@+id/imagebajaj"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:background="@drawable/bajaj" />
            <ImageButton
                android:id="@+id/imagebecak"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:background="@drawable/becak" />
            <ImageButton
                android:id="@+id/imagebis"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:background="@drawable/bis" />
            <ImageButton
                android:id="@+id/imagedelman"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:background="@drawable/delman" />
            <ImageButton
                android:id="@+id/imagekereta"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:background="@drawable/kereta" />

            <ImageButton
                android:id="@+id/imagemotor"
                android:layout_width="200dp"
                android:layout_height="133dp"
                android:background="@drawable/motor" />

            <ImageButton
                android:id="@+id/imagemobil"
                android:layout_width="200dp"
                android:layout_height="143dp"
                android:background="@drawable/mobil" />
            <ImageButton
                android:id="@+id/imagesepeda"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:background="@drawable/sepeda" />
            <ImageButton
                android:id="@+id/imageskuter"
                android:layout_width="200dp"
                android:layout_height="148dp"
                android:background="@drawable/skut" />
            <ImageButton
                android:id="@+id/imagetaxi"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:background="@drawable/taxi" />
            <ImageButton
                android:id="@+id/imagetruk"
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:background="@drawable/truk" />

        </LinearLayout>
    </ScrollView>

</RelativeLayout>

    <ImageView
        android:id="@+id/imageambulan1"
        android:layout_width="380dp"
        android:layout_height="330dp"
        app:srcCompat="@drawable/ambulan1"
        android:layout_marginRight="72dp"
        android:layout_marginEnd="72dp"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/imagebackud"
        android:layout_toStartOf="@+id/imagebackud"
        android:layout_marginBottom="70dp" />
    <ImageButton
        android:id="@+id/imagebackud"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/back"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
