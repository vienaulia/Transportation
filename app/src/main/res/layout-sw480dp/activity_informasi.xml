<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bp"
    android:screenOrientation="landscape"
    tools:context="com.example.vien.transportasi.InformasiActivity">

    <RelativeLayout
        android:layout_width="1200dp"
        android:layout_height="580dp"
        tools:layout_editor_absoluteY="10dp"
        tools:layout_editor_absoluteX="8dp"
        android:id="@+id/relativeLayout2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">

        <ScrollView
            android:layout_width="900dp"
            android:layout_height="320dp"
            android:scrollbars="none"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp"
            android:id="@+id/scrollView2"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <LinearLayout
                android:layout_width="1120dp"
                android:layout_height="550dp"
                android:orientation="vertical" >

                <ImageButton
                    android:id="@+id/menu1"
                    android:layout_width="860dp"
                    android:layout_height="70dp"
                    android:background="@drawable/menu11" />

                <ImageButton
                    android:id="@+id/menu2"
                    android:layout_width="880dp"
                    android:layout_height="180dp"
                    android:background="@drawable/menu22" />
                <ImageButton
                    android:id="@+id/menu3"
                    android:layout_width="860dp"
                    android:layout_height="170dp"
                    android:background="@drawable/menu33" />
                <ImageButton
                    android:id="@+id/menu4"
                    android:layout_width="860dp"
                    android:layout_height="70dp"
                    android:background="@drawable/menu44" />
                <ImageButton
                    android:id="@+id/menu5"
                    android:layout_width="860dp"
                    android:layout_height="65dp"
                    android:background="@drawable/menu55" />
                <ImageButton
                    android:id="@+id/menu6"
                    android:layout_width="860dp"
                    android:layout_height="70dp"
                    android:background="@drawable/menu66" />
                <ImageButton
                    android:id="@+id/menu7"
                    android:layout_width="860dp"
                    android:layout_height="60dp"
                    android:background="@drawable/menu77" />
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>
    <ImageButton
        android:id="@+id/judul"
        android:layout_width="800dp"
        android:layout_height="65dp"
        android:background="@drawable/jp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:id="@+id/copy"
        android:layout_width="460dp"
        android:layout_height="40dp"
        android:background="@drawable/copy"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true" />

    <ImageButton
        android:id="@+id/imagebackin"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@drawable/back"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
